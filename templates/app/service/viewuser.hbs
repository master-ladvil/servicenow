<main class="container mt-5">
    <table class = "table table-sm">
  <thead class="thead">
    <tr>
      <th><td>Id</td></th>
      <th><td>name</td></th>
      <th><td>email</td></th>
      <th><td>uname</td></th>
      <th><td>empno</td></th>
      <th><td>sex</td></th>
      <th><td>phone</td></th>
      <th><td>secphone</td></th>
      <th><td>lang</td></th>
      <th><td>street</td></th>
      <th><td>roles</td></th>
      <th><td>title</td></th>

    </tr>
  </thead>
  <tbody>
   
    {{#each this.model as |row|}}  
      <tr>
        <td>{{row.id}}</td>
        <td>{{row.name}}</td>
        <td>{{row.email}}</td>
        <td>{{row.username}}</td>
        <td>{{row.employeenumber}}</td>
        <td>{{row.gender}}</td>
        <td>{{row.phone}}</td>
        <td>{{row.homephone}}</td>
        <td>{{row.preferredlanguage}}</td>
        <td>{{row.street}}</td> 
        <td>{{row.roles}}</td>
        <td>{{row.title}}</td>
      </tr>
    {{/each}}
    
  </tbody>
</table>
</main>

{{outlet}}